<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.autotrade.dao.SubCustomerDao" >

  <!-- 分页查询所有置换客户 -->
  <select id="selectAll" resultType="com.autotrade.entity.SubCustomer">
    select * from t_substitution limit #{star},#{limit}
  </select>
  <!-- 根据主键id查询 -->
  <select id="selectById" resultType="com.autotrade.entity.SubCustomer">
    select * from t_substitution where id=#{id}
  </select>
  <!-- 根据主键id删除 -->
  <delete id="deleteById">
    delete from t_substitution where id = #{id}
  </delete>
  <!-- 插入全部字段 -->
  <insert id="insertAll" parameterType="com.autotrade.entity.SubCustomer">
    insert into t_substitution 
    	(id,name,phone,address,vehicle_type,brand,service_life,kilometres,
    	 maintenance_record,major_accidents,offer,brands,intention,budget)
    values 
   		(#{id},#{name},#{phone},#{address},#{vehicle_type},#{brand},#{service_life},#{kilometres},
    	#{maintenance_record},#{major_accidents},#{offer},#{brands},#{intention},#{budget})
  </insert>  
  <!-- 插入指定字段 -->
  <insert id="insertSelective" parameterType="com.autotrade.entity.SubCustomer">
    insert into t_substitution
    <trim prefix="(" suffix=")" suffixOverrides=",">
      <if test="id != null">
        id,
      </if>
      <if test="name != null">
        name,
      </if>
      <if test="phone != null">
        phone,
      </if>
      <if test="address != null">
        address,
      </if>
      <if test="vehicle_type != null">
        vehicle_type,
      </if>
      <if test="brand != null">
        brand,
      </if>
      <if test="service_life != null">
        service_life,
      </if>
      <if test="kilometres != null">
        kilometres,
      </if>
      <if test="maintenance_record != null">
        maintenance_record,
      </if>
      <if test="major_accidents != null">
        major_accidents,
      </if>
      <if test="offer != null">
        offer,
      </if>
      <if test="brands != null">
        brands,
      </if>
      <if test="intention != null">
        intention,
      </if>
      <if test="budget != null">
        budget,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides=",">
      <if test="id != null">
        #{id},
      </if>
      <if test="name != null">
        #{name},
      </if>
      <if test="phone != null">
        #{phone},
      </if>
      <if test="address != null">
        #{address},
      </if>
      <if test="vehicle_type != null">
        #{vehicle_type},
      </if>
      <if test="brand != null">
        #{brand},
      </if>
      <if test="service_life != null">
        #{service_life},
      </if>
      <if test="kilometres != null">
        #{kilometres},
      </if>
      <if test="maintenance_record != null">
        #{maintenance_record},
      </if>
      <if test="major_accidents != null">
        #{major_accidents},
      </if>
      <if test="offer != null">
        #{offer},
      </if>
      <if test="brands != null">
        #{brands},
      </if>
      <if test="intention != null">
        #{intention},
      </if>
      <if test="budget != null">
        #{budget},
      </if>
    </trim>
  </insert>
  <!-- 修改全部 -->
  <update id="updateAllById" parameterType="com.autotrade.entity.SubCustomer">
    update t_substitution
    set 
    name = #{name},
    phone = #{phone},
    address = #{address},
    vehicle_type = #{vehicle_type},
    brand = #{brand},
    service_life = #{service_life},
    kilometres = #{kilometres},
    maintenance_record = #{maintenance_record},
    major_accidents = #{major_accidents},
    offer = #{offer},
    brands = #{brands},
    intention = #{intention},
    budget = #{budget}
    where id = #{id}
  </update>
  <!-- 修改指定字段 -->
  <update id="updateByIdSelective" parameterType="com.autotrade.entity.SubCustomer">
    update t_substitution
    <set>
      <if test="name != null">
        name = #{name},
      </if>
      <if test="phone != null">
        phone = #{phone},
      </if>
      <if test="address != null">
        address = #{address},
      </if>
      <if test="vehicle_type != null">
        vehicle_type = #{vehicle_type},
      </if>
      <if test="brand != null">
        brand = #{brand},
      </if>
      <if test="service_life != null">
        service_life = #{service_life},
      </if>
      <if test="kilometres != null">
        kilometres = #{kilometres},
      </if>
      <if test="maintenance_record != null">
        maintenance_record = #{maintenance_record},
      </if>
      <if test="major_accidents != null">
        major_accidents = #{major_accidents},
      </if>
      <if test="offer != null">
        offer = #{offer},
      </if>
      <if test="brands != null">
        brands = #{brands},
      </if>
      <if test="intention != null">
        intention = #{intention},
      </if>
      <if test="budget != null">
        budget = #{budget},
      </if>
    </set>
    where id = #{id}
  </update>

</mapper>
