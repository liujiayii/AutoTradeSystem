package com.autotrade.entity;

import java.util.Date;

/**
 * @ClassName: SellingCustomer
 * @description  卖车客户实体类
 * @author xiaoyuchao
 * @createDate 2019年1月4日
 */
public class SellingCustomer {
	/** 主键id */
	private Long id;
	/** 客户姓名 */
	private String name;
	/** 客户电话 */
	private String phone;
	/** 地址 */
	private String address;
	/** 客户要卖的车型 */
	private String vehicle_type;
	/** 品牌 */
	private String brand;
	/** 使用年限 */
	private String service_life;
	/** 车辆行驶的公里数 单位为公里 */
	private Integer kilometres;
	/** 维修记录 */
	private String maintenance_record;
	/** 有无重大事故（1，代表有，0代表没有，默认为0） */
	private Integer major_accidents;
	/** 粗略报价 */
	private Integer offer;
	/** 创建时间 */
	private Date create_time;
	/** 修改时间 */
	private Date update_time;
	public Long getId() {
		return id;
	}
	public void setId(Long id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getPhone() {
		return phone;
	}
	public void setPhone(String phone) {
		this.phone = phone;
	}
	public String getAddress() {
		return address;
	}
	public void setAddress(String address) {
		this.address = address;
	}
	public String getVehicle_type() {
		return vehicle_type;
	}
	public void setVehicle_type(String vehicle_type) {
		this.vehicle_type = vehicle_type;
	}
	public String getBrand() {
		return brand;
	}
	public void setBrand(String brand) {
		this.brand = brand;
	}
	public String getService_life() {
		return service_life;
	}
	public void setService_life(String service_life) {
		this.service_life = service_life;
	}
	public Integer getKilometres() {
		return kilometres;
	}
	public void setKilometres(Integer kilometres) {
		this.kilometres = kilometres;
	}
	public String getMaintenance_record() {
		return maintenance_record;
	}
	public void setMaintenance_record(String maintenance_record) {
		this.maintenance_record = maintenance_record;
	}
	public Integer getMajor_accidents() {
		return major_accidents;
	}
	public void setMajor_accidents(Integer major_accidents) {
		this.major_accidents = major_accidents;
	}
	public Integer getOffer() {
		return offer;
	}
	public void setOffer(Integer offer) {
		this.offer = offer;
	}
	public Date getCreate_time() {
		return create_time;
	}
	public void setCreate_time(Date create_time) {
		this.create_time = create_time;
	}
	public Date getUpdate_time() {
		return update_time;
	}
	public void setUpdate_time(Date update_time) {
		this.update_time = update_time;
	}
	@Override
	public String toString() {
		return "SellingCustomer [id=" + id + ", name=" + name + ", phone=" + phone + ", address=" + address
				+ ", vehicle_type=" + vehicle_type + ", brand=" + brand + ", service_life=" + service_life
				+ ", kilometres=" + kilometres + ", maintenance_record=" + maintenance_record + ", major_accidents="
				+ major_accidents + ", offer=" + offer + ", create_time=" + create_time + ", update_time=" + update_time
				+ "]";
	}
	
}
